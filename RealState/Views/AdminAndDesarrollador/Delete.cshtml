
@{
    ViewData["Title"] = "Delete";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/css/Alert.css">
    <title>Alert</title>
</head>
<body>
    <div class="container">
        <form asp-controller="AdminAndDesarrollador" asp-action="Delete" class="container">
            <div class="Danger advice">
                <i class="bi bi-exclamation-octagon-fill"></i>
                <p>ESTÁ SEGURO QUE QUIERE ELIMINAR ESTE AGENTE?!!</p>



                <button type="submit" class="btn">ELIMINAR <i class="bi bi-arrow-right-circle-fill"></i></button>
                <button asp-action="Index" class="btn">Volver <i class="bi bi-x-octagon"></i></button>
            </div>
        </form>
    </div>

</body>
</html>